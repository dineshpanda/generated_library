<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Visitor Entries

    <a data-toggle="collapse" href="#user_visitor_entries_new_form">
      Add visitor entry
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.visitor_entries.each do |visitor_entry| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <!-- Show link start -->
        <a href="/visitor_entries/<%= visitor_entry.id %>">
          <%= visitor_entry.visit_date %>
        </a>
        <!-- Show link end -->

      </li>
    <% end %>

  </ul>
</div>
